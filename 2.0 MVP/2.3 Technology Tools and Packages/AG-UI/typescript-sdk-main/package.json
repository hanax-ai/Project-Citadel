{
  "name": "ag-ui",
  "author": "Markus Ecker <markus.ecker@gmail.com>",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:clean": "pnpm -r clean && pnpm install && turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "publish": "pnpm -r clean && pnpm install && turbo run build --filter=./packages/* && pnpm publish -r --filter=./packages/*",
    "bump": "pnpm --filter './packages/*' exec -- pnpm version"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "version": "0.0.1"
}
